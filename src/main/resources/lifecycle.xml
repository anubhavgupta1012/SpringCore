<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"      "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans default-init-method="initm" default-destroy-method="desm">

	<bean id="triangle" class="com.lifecycle.Triangle"
		autowire="byName" scope="prototype">

	</bean>
	<bean id="triangle1" class="com.lifecycle.Triangle1"
		autowire="byName">

	</bean>

	<bean id="pointA" class="com.lifecycle.Points">
		<property name="x" value="0" />
		<property name="y" value="0" />
	</bean>


	<bean id="pointB" class="com.lifecycle.Points">
		<property name="x" value="20" />
		<property name="y" value="0" />
	</bean>

	<bean id="pointC" class="com.lifecycle.Points">
		<property name="x" value="-20" />
		<property name="y" value="0" />
	</bean>
	
	

</beans>